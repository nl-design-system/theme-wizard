{
  "name": "@nl-design-system-community/css-scraper",
  "version": "1.0.1",
  "author": "Community for NL Design System",
  "description": "CSS Scraper",
  "license": "EUPL-1.2",
  "keywords": [
    "expertteam-digitale-toegankelijkheid",
    "nl-design-system"
  ],
  "type": "module",
  "module": "./dist/css-scraper.js",
  "types": "./dist/get-css.d.ts",
  "exports": {
    ".": {
      "types": "./dist/get-css.d.ts",
      "import": "./dist/css-scraper.js"
    }
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/theme-wizard.git",
    "directory": "packages/css-scraper"
  },
  "scripts": {
    "start": "vite preview",
    "dev": "vite",
    "lint": "pnpm run --sequential '/^lint:.+$/'",
    "lint:js": "eslint",
    "lint-fix:js": "eslint --fix",
    "test-build": "vitest run --coverage",
    "build": "vite build",
    "clean": "rimraf dist/"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.15",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.8"
  },
  "dependencies": {
    "@nl-design-system-community/design-tokens-schema": "workspace:*",
    "@projectwallace/css-design-tokens": "0.10.0",
    "linkedom": "0.18.12",
    "zod": "4.1.12"
  }
}
