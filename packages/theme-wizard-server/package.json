    "vercel": "50.22.1",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "@hono/node-server": "1.19.9",
    "@hono/swagger-ui": "0.5.3",
    "@hono/zod-openapi": "1.2.2",
    "@nl-design-system-community/css-scraper": "workspace:*",
