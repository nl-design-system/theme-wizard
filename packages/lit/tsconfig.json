{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["ES2022", "DOM"],
    "strict": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "types": [],
    "importHelpers": false,
    "useDefineForClassFields": false,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "vite-env.d.ts"]
}
