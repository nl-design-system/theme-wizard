{
  "name": "@nl-design-system-community/clippy-storybook",
  "version": "1.0.0",
  "author": "Community for NL Design System",
  "description": "Web server with REST API for scraping websites to extract design tokens",
  "license": "EUPL-1.2",
  "keywords": [
    "expertteam-digitale-toegankelijkheid",
    "nl-design-system"
  ],
  "private": true,
  "exports": {
    "./patterns/*": {
      "import": "./dist/patterns/*/index.js",
      "types": "./dist/patterns/*/index.d.ts"
    }
  },
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/example.git",
    "directory": "packages/clippy-storybook"
  },
  "scripts": {
    "dev": "pnpm run storybook --no-open",
    "build": "storybook build --config-dir ./config/ --output-dir ./dist/",
    "build:components": "vite build",
    "clean": "rimraf ./dist/",
    "lint": "pnpm run --sequential '/^lint:.+$/'",
    "lint:js": "eslint",
    "lint-fix:js": "eslint --fix",
    "storybook": "storybook dev --config-dir ./config/ --port 6006"
  },
  "devDependencies": {
    "@nl-design-system-community/clippy-components": "workspace:*",
    "@nl-design-system-community/ma-design-tokens": "3.0.0",
    "@nl-design-system-community/theme-wizard-app": "workspace:*",
    "@nl-design-system/tsconfig": "1.0.5",
    "@rijkshuisstijl-community/storybook-tooling": "1.1.1",
    "@storybook/addon-a11y": "10.1.11",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/react": "9.1.5",
    "@storybook/react-vite": "9.1.5",
    "@storybook/web-components-vite": "10.1.11",
    "@tabler/icons-react": "3.36.0",
    "@types/lodash-es": "4.17.12",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@utrecht/component-library-react": "10.3.10",
    "@whitespace/storybook-addon-html": "8.0.2",
    "glob": "13.0.0",
    "lit": "3.3.2",
    "lodash-es": "4.17.22",
    "prettier": "3.7.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "10.1.11",
    "vite": "7.3.0",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.16"
  }
}
